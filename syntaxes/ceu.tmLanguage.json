{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "CÃ©u",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#entities"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#storages"
		},
		{
			"include": "#line-comments"
		},
		{
			"include": "#block-comments"
		}
	],
	"repository": {
		"line-comments": {
			"name": "comment.line.double-slash.ceu",
			"begin": "//",
			"end": "\n",
			"patterns": [{
				"name": "constant.character.escape.ceu",
				"match": "\\\\."
			}]			
		},
		"block-comments": {
			"name": "comment.block.ceu",
			"begin": "/\\*",
			"end": "\\*/",
			"patterns": [{
				"name": "constant.character.escape.ceu",
				"match": "\\\\."
			}]			
		},		
		"keywords": {
			"patterns": [{
				"name": "keyword.control.ceu",
				"match": "\\b(do|end|finaliza|await|emit|watching|every|if|then|else|else/if|loop|escape|code|call|var|event|output|input|val|data|par/or|par/and|par|with|spawn|in)\\b"
			}]
		},
		"entities": {
			"patterns": [{
				"name": "entity.name.function",
				"match": "asd([A-Z])+"
			}]
		},
		"operators": {
			"patterns": [{
				"name": "keyword.operator.ceu",
				"match": "(\\+|\\-(?!>)|\\*|(\\/(?!(\\/|\\*)))|\\%)"
			}]
		},"constants": {
			"patterns": [{
				"name": "constant.other.ceu",
				"match": "\\b(COLOR_([A-Z])+)\\b"
			},{
				"name": "constant.language.ceu",
				"match": "\\b(true|false|yes|no|NEVER|FOREVER|none|null)\\b"
			},{
				"name": "constant.numeric.ceu",
				"match": "((\\d)+(s|ms))|((?<=[\\s\\-\\+\\*\\\/\\\\=:\\[\\(\\{,]|^)\\d*\\.?\\d+([eE][+-]?\\d)?([0-9&&[^\\.]])*(i|j)?)"
			}]
		},
		"strings": {
			"name": "string.quoted.double.ceu",
			"begin": "\"",
			"end": "\"",
			"patterns": [{
				"name": "constant.character.escape.ceu",
				"match": "\\\\."
			}]
		},
		"storages": {
			"patterns": [{
				"name": "storage.type.ceu",
				"match": "\\b(int|integer|bool|pool|uint|u8|u16|u32|u64|s8|s16|s32|s64)\\b"
			}]
		}
	},
	"scopeName": "source.ceu"
}